@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "tMemberMaster.cshtml";
    var AdvertisementID = ConvertToInt(@Request.QueryString["ID"]);
}

@functions {
    public int? ConvertToInt(string data)
    {
        return !String.IsNullOrEmpty(data) ? Convert.ToInt32(data) : (int?)null;
    }
}

@Html.Action("Details", "Advertisement", new { id = AdvertisementID })